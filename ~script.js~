var chooseElement;

const move = function(draggableimg){
  const elements = document.querySelectorAll("draggableimg");

  elements.forEach((draggableimg) => {
    draggableimg.addEventListener("mousedown", () => {
      draggableimg.style.position = "absolute";
      chooseElement = draggableimg;

      document.onmousemove = (e) => {
        var x = e.pageX
        var y = e.pageY

        chooseElement.style.left = x 0  + "px"
        chooseElement.style.left = y 0 + "px"

        console.log(chooseElement)
      };
    });
  });
  document.onmouseup = function (e){chooseElement = null}

};